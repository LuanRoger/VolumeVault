{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "PostgresConnectionString": "User ID={0};Password={1};Host={2};Port={3};Database={4};",
    "MongoDBConnectionString": "mongodb://{0}:{1}@{2}:{3}/"
  }
}
